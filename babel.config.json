{
  "presets": [
    "@react-native/babel-preset"
  ],
  "plugins": [
    "react-native-reanimated/plugin",
    "jotai/babel/plugin-react-refresh",
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "@app": "./src"
        }
      }
    ],
    [
    "formatjs",
      {
        "idInterpolationPattern": "[sha512:contenthash:base64:6]",
        "ast": true
      }
    ]
  ]
}
