{
  "name": "jenkins-status-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "yarn pods",
    "pods": "run-script-os",
    "pods:macos": "pod-install ios && pod-install macos",
    "pods:windows": "echo No pods to install on Windows",
    "lint": "eslint .",
    "test": "jest",
    "start": "react-native start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "start:macos": "react-native run-macos",
    "start:windows": "react-native run-windows",
    "reset": "run-script-os",
    "reset:macos": "yarn clean:android && yarn clean:ios && yarn clean:macos && yarn clean:node && yarn",
    "reset:windows": "yarn clean:android && yarn clean:windows && yarn clean:node && yarn",
    "clean:node": "shx rm -rf node_modules",
    "clean:ios": "shx rm -rf ios/build ios/Pods ios/Podfile.lock",
    "clean:android": "shx rm -rf android/build",
    "clean:macos": "shx rm -rf macos/build macos/Pods macos/Podfile.lock",
    "clean:windows": "shx rm -rf windows/packages windows/x64"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "^1.79.2",
    "@tamagui/config": "^1.79.2",
    "@tamagui/lucide-icons": "^1.79.2",
    "@tamagui/shorthands": "^1.79.2",
    "@tamagui/themes": "^1.79.2",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-macos": "^0.72.0-0",
    "react-native-svg": "^14.0.0",
    "react-native-web": "^0.19.9",
    "react-native-windows": "^0.72.0-0",
    "tamagui": "^1.79.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-config": "^0.76.8",
    "metro-react-native-babel-preset": "0.76.8",
    "normalize-path": "^3.0.0",
    "pod-install": "^0.2.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "run-script-os": "^1.1.6",
    "shx": "^0.3.4",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
